  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd app && npm install && npm run build",
  "installCommand": "cd app && npm install",
  "outputDirectory": "app/.next",
  "env": {
    "NEXT_PUBLIC_RPC_URL": "https://api.devnet.solana.com",
    "NEXT_PUBLIC_NETWORK": "devnet",
    "NODE_ENV": "production"
  }
}
